{
  "compilerOptions": {
    "outDir": "./build/",
    "noImplicitAny": false,
    "removeComments": true,
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "jsx": "react",
    "allowJs": true,
    "jsxFactory": "h",
    "moduleResolution": "node",
  },
  "suppressImplicitAnyIndexErrors": true,
  "lib": [
    "dom",
    "es2017"
  ],
  "baseUrl": ".",
  "exclude": [
    "dist"
  ]
}