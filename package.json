{
  "name": "prusa-connect-web",
  "version": "1.0.0",
  "description": "This is a printer connect webservice. It is compatible with another 3D printer services, so many applications which use this service.",
  "private": true,
  "scripts": {
    "api:dev": "swagger-repo serve",
    "api:test": "swagger-repo validate",
    "ui:dev": "node ./scripts/proxy.js & parcel ui/src/index.html",
    "ui:build": "parcel build ui/src/index.html --no-source-maps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prusa3d/Prusa-Connect-Web.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/prusa3d/Prusa-Connect-Web/issues"
  },
  "homepage": "https://github.com/prusa3d/Prusa-Connect-Web#readme",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "http-proxy": "^1.18.0",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.24.2",
    "swagger-repo": "^2.0.0-rc.15",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "bulma": "^0.8.0",
    "preact": "^10.1.1",
    "preact-jsx-chai": "^2.3.2",
    "preact-markup": "^1.6.0",
    "preact-render-to-string": "^5.1.3",
    "preact-router": "^3.1.0"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat"
  }
}
