{
  "name": "prusa-connect-web",
  "version": "1.0.0",
  "description": "This is a printer connect webservice. It is compatible with another 3D printer services, so many applications which use this service.",
  "private": true,
  "scripts": {
    "api:start": "swagger-repo serve",
    "api:test": "swagger-repo validate",
    "ui:build": "npm run ui:clean && npx webpack --config ./webpack.config.js --mode production --env.apiKey=developer --env.printer='Prusa SL1' --progress",
    "ui:dev": "npm run ui:clean && npx webpack --config ./webpack.config.js --mode development",
    "ui:start": "npx webpack-dev-server --config ./webpack.config.js --env.prod=1 --mode development",
    "ui:clean": "rimraf ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prusa3d/Prusa-Connect-Web.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/prusa3d/Prusa-Connect-Web/issues"
  },
  "homepage": "https://github.com/prusa3d/Prusa-Connect-Web#readme",
  "devDependencies": {
    "autoprefixer": "^9.7.3",
    "bulma": "^0.8.0",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "favicons-webpack-plugin": "^2.1.0",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "glob-all": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.20.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "preact-jsx-chai": "^2.3.2",
    "preact-markup": "^1.6.0",
    "preact-render-to-string": "^5.1.3",
    "preact-router": "^3.1.0",
    "purgecss-webpack-plugin": "^1.6.0",
    "rimraf": "^3.0.0",
    "sass": "^1.24.2",
    "sass-loader": "^8.0.1",
    "svg-url-loader": "^3.0.3",
    "swagger-repo": "^2.0.0-rc.15",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "preact": "^10.1.1"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat"
  }
}
